{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://192.168.*"
        },
        {
          "url": "http://localhost:3002"
        },
        {
          "url": "https://*.tauri.app"
        }
      ]
    },
    "os:default",
    "deep-link:default",
    "core:event:default",
    "opener:default"
  ]
}